{
  "name": "Kindle Checker Importer",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Importer for Kindle Edition Checker",
  "permissions": [
    "tabs",
    "contextMenus",
    "clipboardRead",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [{
    "matches": ["https://www.amazon.co.jp/*"],
    "js": ["dist/script.js"]
  }],
  "background": {
    "scripts": ["dist/background.js"]
  }
}
